{
  "build_instructions": {
    "name": "prednet_example",
    "load_from": "rotnet_example.h5",
    "keep_until": -3,
    "cnn_layers": [[64, 3]],
    "input_shape": [32, 32, 3],
    "backbone_cnn_layers": [[96, 16], [128, 8],[128, 8]],
    "include_maxpool": true,
    "dense_layers": [128],
    "num_classes": 10,
    "transfer": false
  },
  "training": {
    "epochs": 1,
    "learning_rate": 0.01,
    "batch_size": 64,
    "verbose": 2,
    "val_split" :0.2,
    "optimizer": "Adam",
    "loss" : "SparseCategoricalCrossEntropy",
    "shuffle" : true
  },
  "save_path": "prednet_example.h5"
}
