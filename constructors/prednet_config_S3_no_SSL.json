{
  "build_instructions": {
    "name": "prednet_Small",
    "load_from": "rotnet_3.h5",
    "keep_until": -3,
    "cnn_layers": [[64, 6], [128, 3]],
    "include_maxpool": true,
    "dense_layers": [128],
    "num_classes": 10,
    "transfer": false

  },
  "training": {
    "epochs": 10,
    "learning_rate": 0.001,
    "batch_size": 64,
    "verbose": 2,
    "val_split" :0.2,
    "optimizer": "Adam",
    "loss" : "SparseCategoricalCrossEntropy",
    "shuffle" : true,
    "dataset_size" : 10
  },
  "save_path": "prednet_Small_SSL_3.h5"
}
