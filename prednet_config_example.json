{
  "build_instructions": {
    "name": "prednet_example",
    "load_from": "rotnet_example.h5",
    "keep_until": -3,
    "cnn_layers": [[64, 3]],
    "include_maxpool": true,
    "dense_layers": [128],
    "num_classes": 10
  },
  "training": {
    "epochs": 20,
    "learning_rate": 0.003,
    "batch_size": 64,
    "verbose": 2,
    "val_split" :0.2,
    "optimizer": "Adam",
    "loss" : "SparseCategoricalCrossEntropy",
    "shuffle" : true
  },
  "save_path": "prednet_example.h5"
}
