{
  "build_instructions": {
    "name": "prednet_S5_SSL",
    "load_from": "rotnet_5.h5",
    "keep_until": -12,
    "cnn_layers": [[32, 6], [64, 6], [128, 6]],
    "include_maxpool": false,
    "dense_layers": [128],
    "num_classes": 10,
    "transfer": true

  },
  "training": {
    "epochs": 150,
    "learning_rate": 0.001,
    "batch_size": 64,
    "verbose": 2,
    "val_split" :0.2,
    "optimizer": "Adam",
    "loss" : "SparseCategoricalCrossEntropy",
    "shuffle" : true,
    "dataset_size" : 5000
  },
  "save_path": "prednet_S5_SSL.h5"
}
